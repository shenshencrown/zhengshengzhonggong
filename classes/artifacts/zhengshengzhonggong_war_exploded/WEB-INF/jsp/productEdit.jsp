<script type="text/javascript">
    var myEditor = UE.getEditor('editor');
    myEditor.ready(function(){
        myEditor.setDisabled();
        myEditor.setHide();  //隐藏编辑器

        myEditor.addListener('beforeInsertImage',function(t,arg){ //侦听图片上传
            var imagesUrl="";
            for (var i = 0; i < arg.length; i++) { //实现多图上传. ,分割图片地址
                if(arg[i].src === undefined){
                    continue;
                }
                if(i == (arg.length-1)){
                    imagesUrl=imagesUrl+(arg[i].src);
                    //imagesUrl=imagesUrl.replace("/../../","../");
                    imagesUrl=imagesUrl.replace("/../../","/");
                }else{
                    imagesUrl=imagesUrl+(arg[i].src); //
                    //imagesUrl=imagesUrl.replace("/../../","../")+"|";
                    imagesUrl=imagesUrl.replace("/../../","/")+"|";
                }
            }
            //myEditorImage.close();
            //document.getElementById("d_image_savepath").value=imagesUrl.replace("/../../","../");//将地址赋值给相应的input
            document.getElementById("otherpic").value=imagesUrl.replace("/../../","/");//将地址赋值给相应的input
            //alert(imagesUrl);
        })
        /*
            需要在dialogs\attachment\attachment.html文件中119行   editor.execCommand("insertHTML",str);前一行添加editor.fireEvent('afterUpfile', filesList);,才可以用下面的addListener
    */
        myEditor.addListener('afterUpfile',function(t,arg){
            //alert(myEditor.options.filePath );//取相对路径
            //alert(arg[0].url);
            $("#files").attr("value",myEditor.options.filePath+arg[0].url); // 多文件上传只显示第一个,可修改.上传文件返回对象有url,original,type.
        })
    })

    function upImage(){
        var myEditorImage = myEditor.getDialog("insertimage");
        myEditorImage.open();     //打开dialog
        alert(imagesUrl);
    }

    function upFiles(){
        var myEditorFiles = myEditor.getDialog("attachment");
        myEditorFiles.open();
        // myEditorFiles.close();
    }
</script>
